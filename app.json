{
  "expo": {
    "name": "Dominoes App",
    "slug": "dominoes-app",
    "scheme": "dominoes-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/dominoes-icon-light.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/dominoes-icon-light.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/dominoes-icon-light.png"
    },
    "plugins": ["expo-router"]
  }
}
